{
  "name": "neurapath-homepage",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "license": "MIT",
  "packageManager": "bun@1.3",
  "description": "Neurapath homepage - Beautiful and powerful learning platform built with SvelteKit and deployed on Cloudflare Workers",
  "main": "svelte.config.js",
  "keywords": [
    "learning",
    "education",
    "flashcards",
    "spaced-repetition",
    "sveltekit",
    "cloudflare"
  ],
  "author": "Martin Jakobsson <Hello@neurapath.shop>",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "cf-typegen": "wrangler types ./src/worker-configuration.d.ts",
    "deploy": "bun run build && bun run cf-typegen && wrangler deploy --minify",
    "clean": "rm -rf .svelte-kit && rm -rf node_modules && rm -rf .wrangler && rm -rf .svelte-kit && rm -f bun.lock && rm -f package-lock.json && bun i"
  },
  "devDependencies": {
    "@lucide/svelte": "^0.553.0",
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/kit": "^2.48.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "clsx": "^2.1.1",
    "svelte": "^5.43.5",
    "svelte-check": "^4.3.3",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.17",
    "tslib": "^2.8.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "wrangler": "^4.46.0"
  },
  "dependencies": {
    "posthog-js": "^1.290.0",
    "posthog-node": "^5.11.2"
  }
}